{
	"LocalAxis":
	{
		"comment": "ボーンのローカル軸を表す",
		"notice": "Z手前，X右，Y上",
		"variables":
		{
			"boneId":
			{
				"type": "Int",
				"comment": "ローカル軸のあるボーンID"
			},
			"AxisX":
			{
				"type": "Vector3",
				"comment": "X軸の向き"
			},
			"AxisZ":
			{
				"type": "Vector3",
				"comment": "Z軸の向き"
			}
		}
	},
	
	"LocalAxisList":
	{
		"comment": "ローカル軸のリスト",
		"extends": "ArrayList",
		"parameters": ["LocalAxis"]
	},
	
	"Limit":
	{
		"comment": "ボーンの動作制限",
		"variables":
		{
			"limitType":
			{
				"type": "Enum",
				"comment": "制限の種類",
				"enum": ["Position", "Rotation", "Scale"]
			},
			"maxLimit":
			{
				"type": "UnitVector3",
				"comment": "最大の制限値，limitTypeによって意味は変わる"
			},
			"minLimit":
			{
				"type": "UnitVector3",
				"comment": "最小の制限値，limitTypeによって意味は変わる"
			}
		}
	},
	
	"LimitList":
	{
		"comment": "制限のリスト",
		"extends": "ArrayList",
		"parameters": ["Limit"]
	},
	
	"Constraint":
	{
		"comment": "拘束のベースクラス，sourceがtargetに対して拘束する",
		"variables":
		{
			"sourceBoneId":
			{
				"type": "Int",
				"comment": "拘束するボーンのID"
			},
			"targetBoneId":
			{
				"type": "Int",
				"comment": "拘束されるボーンのID"
			},
			"ratio":
			{
				"type": "Float",
				"comment": "拘束をかける割合"
			},
			"constraintType":
			{
				"type": "Enum",
				"comment": "拘束の種類",
				"enum":
				[
					"Point", "Orient", "Parent", "AimX", "AimY", "AimZ"
				]
			}
		}
	},
	
	"ConstraintList":
	{
		"comment": "拘束のリスト",
		"extends": "ArrayList",
		"parameters": ["Constraint"]
	},
	
	"IK":
	{
		"comment": "IK",
		"variables":
		{
			"rootBoneId":
			{
				"type": "Int",
				"comment": "IKの根本のボーンID"
			},
			"tailBoneId":
			{
				"type": "Int",
				"comment": "IKの末端のボーンID"
			},
			"iteration":
			{
				"type": "Int",
				"comment": "計算回数"
			},
			"rotateLimitation":
			{
				"type": "Float",
				"comment": "1フレームあたりに回転可能な角度",
				"notice": "ラジアン角, 30FPS換算"
			}
		}
	},
	
	"IKList":
	{
		"comment": "IKのリスト",
		"extends": "ArrayList",
		"parameters": ["IK"]
	}
}