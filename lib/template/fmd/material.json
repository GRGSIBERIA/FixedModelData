{
	"TexturePath":
	{
		"comment": "テクスチャのファイルパス",
		"extends": "FixedArrayList",
		"parameters": ["wchar_t", "256"]
	},
	
	"TextureList":
	{
		"comment": "テクスチャのリスト",
		"extends": "ArrayList",
		"parameters": ["TexturePath"]
	},
	
	"Material":
	{
		"comment": "マテリアル",
		"variables":
		{
			"name":
			{
				"type": "FixedArrayList",
				"comment": "マテリアル名",
				"notice": "ASCII文字列",
				"parameters": ["char", "40"]
			},
			"diffuseColor":
			{
				"type": "ColorRGBA",
				"comment": "拡散色"
			},
			"specularColor":
			{
				"type": "ColorRGB",
				"comment": "反射色"
			},
			"specularity":
			{
				"type": "Float",
				"comment": "反射係数"
			},
			"ambientColor":
			{
				"type": "ColorRGB",
				"comment": "環境色"
			},
			"renderFlag":
			{
				"type": "Flag",
				"comment": "レンダリングの設定",
				"enum": [
					"BackFaceCulling", "ProjectionShadow", "UseShadowMap", "SelfShadow"
				]
			}
		}
	},
	
	"AssignedEdge":
	{
		"comment": "マテリアルにアサインされているエッジ",
		"variables":
		{
			"materialId":
			{
				"type": "Int",
				"comment": "アサインしたいマテリアルのID"
			},
			"edgeColor":
			{
				"type": "ColorRGB",
				"comment": "エッジの描画色",
				"notice": "処理系によっては不必要"
			},
			"edgeSize":
			{
				"type": "Float",
				"comment": "エッジの大きさ",
				"notice": "処理系によっては不必要"
			}
		}
	},
	
	"AssignedEdgeList":
	{
		"comment": "マテリアルにアサインされているエッジのリスト",
		"extends": "ArrayList",
		"parameters": ["AssignedEdge"]
	},
	
	"AssignedTexture":
	{
		"comment": "マテリアルにアサインされているテクスチャ",
		"variables":
		{
			"materialId":
			{
				"type": "Int",
				"comment": "アサインしたいマテリアルのID"
			},
			"textureId":
			{
				"type": "Int",
				"comment": "アサインするテクスチャのID"
			}
		}
	},
	
	"AssignedTextureList":
	{
		"comment": "マテリアルにアサインされたテクスチャのリスト",
		"extends": "ArrayList",
		"parameters": ["AssignedTexture"]
	},
	
	"AssignedSphereMap":
	{
		"comment": "マテリアルにアサインされているスフィアマップ",
		"extends": "AssignedTexture",
		"variables":
		{
			"sphereMode":
			{
				"type": "Enum",
				"comment": "スフィアマップの動作",
				"notice": "拡張子の種類に関わらず作動",
				"enum": ["Disable", "Addition", "Subtraction"]
			}
		}
	},
	
	"AssignedSphereMapList":
	{
		"comment": "マテリアルにアサインされたスフィアマップのリスト",
		"extends": "ArrayList",
		"parameters": ["AssignedSphereMap"]
	},
	
	"AssignedNormalMap":
	{
		"comment": "マテリアルにアサインされているノーマルマップ",
		"extends": "AssignedTexture"
	},
	
	"AssignedNormalMapList":
	{
		"comment": "マテリアルにアサインされているノーマルマップのリスト",
		"extends": ["ArrayList"],
		"parameters": ["AssignedNormalMap"]
	},
	
	"AssignedBumpMap":
	{
		"comment": "マテリアルにアサインされているバンプマップ",
		"extends": "AssignedTexture"
	},
	
	"AssignedBumpMapList":
	{
		"comment": "マテリアルにアサインされているバンプマップのリスト",
		"extends": ["ArrayList"],
		"parameters": ["AssignedBumpMap"]
	}
}