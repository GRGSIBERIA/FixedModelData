{
	"Bone":
	{
		"comment": "ボーン情報",
		"variables":
		{
			"name":
			{
				"type": "FixedArrayList",
				"parameters": ["string", 40],
				"comment": "ボーンの名前",
				"notice": "ASCIIコード，パディングは0詰め"
			},
			"position":
			{
				"type": "UnitVector3",
				"comment": "ボーンの位置"
			},
			"parentId":
			{
				"type": "Int",
				"comment": "親ボーンのID",
				"notice": "0より小さい値はなし"
			},
			"unlockFlag":
			{
				"type": "Flag",
				"comment": "制御可能な方法",
				"enum":
				[
					"Translate", "Rotate", "Scale", "Visible"
				]
			},
			"nodeType":
			{
				"type": "Enum",
				"comment": "どの部位のボーンかを表す",
				"enum":
				[
					"Head", "Neck",
					"Sholder", "Arm", "ArmRoll", "ForeArm", "ForeArmRoll", "Hand",
					"Thumb", "IndexFinger", "MiddleFinger", "RingFinger", "LittleFinger",
					"Spine",
					"Hips",
					"UpLeg", "UpLegRoll", "Leg", "LegRoll", "Foot", "Toe",
					"Eyebrow", "Mouth", "Eye", "Cheek", "Jaw"
				]
			},
			"directionType":
			{
				"type": "Enum",
				"comment": "nodeTypeに対して，どの方向のものかを表す",
				"enum":
				[
					"None", "Left", "Right", "Upper", "Lower"
				]
			}
		}
	},
	
	"Weight":
	{
		"comment": "1頂点と1ボーンに対するウェイト",
		"variables":
		{
			"boneId": 
			{
				"type": "Int",
				"comment": "ボーン番号"
			},
			"value":
			{
				"type": "Float",
				"comment": "ウェイト値"
			}
		}
	},
	
	"WeightList":
	{
		"comment": "1頂点に対する複数のウェイトをまとめる",
		"extends": "ArrayList",
		"parameters": ["Weight"]
	},
	
	"BoneWeight":
	{
		"comment": "頂点に対するボーンのウェイト値を格納する，サイズ頂点数と同じ",
		"extends": "ArrayList",
		"parameters": ["WeightList"],
		"variables":
		{
			"numberOfInfluences":
			{
				"type": "Int",
				"comment": "影響ボーン数，全て同じ数になる"
			}
		}
	}
}